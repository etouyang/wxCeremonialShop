 .shop-container-detail {
    display: flex;
    margin: 30rpx 30rpx 0;
    width: calc( 100% - 60rpx );
  }
  .shop-container-item {
    flex: 1;
    width: 50%;
  }
  .shop-left-item {
    width: calc( 100% - 14rpx);
    margin: 0 14rpx 30rpx 0;
    height: auto;
  }
  .shop-right-item {
    width: calc( 100% - 14rpx);
    margin: 0 0 30rpx 14rpx;
    height: auto;
  }
  .mask{
    width:100%;
    height:100%;
    position:fixed;
    background-color:#999;
    z-index:9000;
    top:0;
    left:0;
    opacity:0.5;
    display: flex;
  }
  .maskContainer {
      width: 100%;
      height: 900rpx;
      background-color: red;
      position: fixed;
      z-index: 9001;
  }
  .whiteContainer {
      width: 100%;
      height: 100%;
      background-color: #fff;
      border-top-left-radius: 20rpx;
      border-top-right-radius: 20rpx;
      display: flex;
      flex-direction: column;
  }
  .maskHead {
      width: 100%;
      height: 190rpx;
      margin-top: -30rpx;
      display: flex;
      flex-direction: row;
  }
  .headImgContainer {
      width: 185rpx;
      height: 185rpx;
      margin-left: 30rpx;
      box-shadow: 0rpx 10rpx 50rpx 10rpx rgba(85, 85, 85, 0.10);
      display: flex;
      align-items: center;
      justify-content: center;
      border-radius: 10rpx;
  }
  .headImg {
      width: 130rpx;
      height: 130rpx;
      background-color: gray;
      border-radius: 65rpx;
  }
  .priceHeadContainer {
    flex: 1;
    margin-top: 30rpx;
    height: calc( 100% - 30rpx );
    display: flex;
    flex-direction: column;
    background-color: rebeccapurple;
  }
  .priceTop {
    width: 100%;
    height: 100rpx;
    justify-content: space-between;
    display: flex;
  }
  .price {
    margin-left: 25rpx;
    margin-top:25rpx;
  }
  .current {
    display: block;
    color: red;
    font-size: 10px;
  }
  .currentNumber {
    font-size: 16px;
  }
  .before {
    display: block;
    color: #666;
    font-size: 10px;
    margin-top: -10rpx;
    text-decoration:line-through;
  }
  .close {
    width: 60rpx;
    height: 60rpx;
  }
  .closeImg {
    margin-top: 20rpx;
    width: 40rpx;
    height: 40rpx;
  }
  .select {
    width: calc( 100% - 50rpx );
    height: 60rpx;
    background-color: red;
    margin: 0 25rpx;
    display: flex; 
    align-items: flex-end;
}
.selectText {
    width: 500rpx;
    height: 100%;
    font-size: 13px;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
  }
.maskSize {
    width: 100%;
    margin-top: 30rpx;
}
.maskTitle {
    color: #000;
    font-size: 15px;
    margin-left: 30rpx;
    margin-bottom: -5rpx;
}
