.container {
  width: 100%;
  position: absolute;
  display: flex;
  align-items: flex-end;
}
.nav {
  display: flex;
  width: 100%;
  height: 80rpx;
  position: relative;
  flex-direction: row;
  /* justify-content: space-between; */
  align-items: center;
}
.leftContainer {
  flex: 1;
  height: 90rpx;
  display: flex;
  align-items: center;
}
.leftImg {
  width: 45rpx;
  height: 45rpx;
  margin-left: 30rpx;
}
.titleContainer {
  width: 150px;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 18px;
  font-weight: bold;
}
.rightContainer {
  /* flex: 1; */
  width: 120px;
  height: 90rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.rightImg {
  width: 45rpx;
  height: 45rpx;
  margin-right: 30rpx;
}