    justify-content: center;
}
.more {
    width: 50rpx;
    height: 50rpx;
    border-radius: 25rpx;
    margin-top: 25rpx;
    background-color: red;
}
.sure {
    position: absolute;
    bottom: 10rpx;
    left: 30rpx;
    right: 30rpx;
