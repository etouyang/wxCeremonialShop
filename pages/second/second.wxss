page {
    background-color: #F4F4F4;
}
.container {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
}
.headContainer {
    width: 100%;
    height: 430rpx;
    display: flex;
}
.headContainer::after {
    content: '';
    width: 160%;
    height: 430rpx;
    position: absolute;
    left: -30%;
    top: 0;
    z-index: -1;
    border-radius: 0 0 50% 50%;
    background: linear-gradient(to right, #FF7854, #FF353F);
}
.timeContainer {
    margin-top: 190rpx;
    width: 100%;
}
.secondItemContainer {
    width: 100%;
    height: auto;
    display: flex;
    flex-direction: column;
    margin-top: 40rpx;
}
.secondItem {
    width: calc( 100% - 30rpx * 2 );
    height: 200rpx;
    background-color: #fff;
    border-radius: 10rpx;
    margin: 0 30rpx 25rpx;
}