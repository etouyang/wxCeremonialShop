    height: 80rpx;
}
.step-item {
    width: 25%;
    font-size: 12px;
    color: #666;
    display: flex;
}
.inline-item {
    flex: 1;
    align-items: center;
    justify-content: center;
    display: flex;
    flex-direction: column;
}
.inline-go {
    width: 10rpx;
    align-items: center;
    justify-content: center;
    display: flex;
}